          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_NURMQJiuEeilzLp3awIxoQ" name="IAProp" specification="_NULssJiuEeilzLp3awIxoQ">
            <language>JAVA</language>
            <body>&#xD;
    String prop = &quot;&quot;;&#xD;
    char r = ' ';&#xD;
    char p = ' ';&#xD;
    for (int i = 0; i &lt; rep.length(); i++) {&#xD;
        r = rep.charAt(i);&#xD;
        p = lastProp.charAt(i);&#xD;
        if (p == '5') {&#xD;
            if (r == '=') {&#xD;
                prop = prop + '5';&#xD;
            } else if (r == '+') {&#xD;
                prop = prop + '8';&#xD;
            } else {&#xD;
                prop = prop + '2';&#xD;
            }&#xD;
        }&#xD;
        if (p == '2') {&#xD;
            if (r == '=') {&#xD;
                prop = prop + '2';&#xD;
            } else if (r == '+') {&#xD;
                prop = prop + '3';&#xD;
            } else {&#xD;
                prop = prop + '1';&#xD;
            }&#xD;
        }&#xD;
        if (p == '8') {&#xD;
            if (r == '=') {&#xD;
                prop = prop + '8';&#xD;
            } else if (r == '+') {&#xD;
                prop = prop + '9';&#xD;
            } else {&#xD;
                prop = prop + '7';&#xD;
            }&#xD;
        }&#xD;
        if (p == '3') {&#xD;
            if (r == '=') {&#xD;
                prop = prop + '3';&#xD;
            } else if (r == '+') {&#xD;
                prop = prop + '4';&#xD;
            } else {&#xD;
                prop = prop + '2';&#xD;
            }&#xD;
        }&#xD;
        if (p == '7') {&#xD;
            if (r == '=') {&#xD;
                prop = prop + '7';&#xD;
            } else if (r == '+') {&#xD;
                prop = prop + '8';&#xD;
            } else {&#xD;
                prop = prop + '6';&#xD;
            }&#xD;
        }&#xD;
        if (p == '1') {&#xD;
            if (r == '=') {&#xD;
                prop = prop + '1';&#xD;
            } else if (r == '+') {&#xD;
                prop = prop + '2';&#xD;
            } else {&#xD;
                prop = prop + '0';&#xD;
            }&#xD;
        }&#xD;
        if (p == '9') {&#xD;
            if (r == '=') {&#xD;
                prop = prop + '9';&#xD;
            } else {&#xD;
                prop = prop + '8';&#xD;
            }&#xD;
        }&#xD;
    }&#xD;
    lastProp = prop;&#xD;
    return prop;&#xD;
</body>
