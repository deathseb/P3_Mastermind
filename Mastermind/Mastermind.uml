            <body>&#xD;
    // TODO Auto-generated method stub&#xD;
    //initialiserPM();&#xD;
    JFrame frame = new JFrame();&#xD;
    JPanel pan = new JPanel();&#xD;
    JPanel panMaster = new JPanel();&#xD;
    JPanel panPlus = new JPanel();&#xD;
    JPanel panQuitter = new JPanel();&#xD;
    JButton mastermind = new JButton(&quot;Jouer au MasterMind&quot;);&#xD;
    JButton plusMoins = new JButton(&quot;Jouer au + / -&quot;);&#xD;
    JButton quitter = new JButton(&quot;Quitter&quot;);&#xD;
    frame.setTitle(&quot;P3 - Mettez votre logique à l'épreuve&quot;);&#xD;
    frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);&#xD;
    frame.setSize(300, 600);&#xD;
    frame.setLocationRelativeTo(null);&#xD;
    pan.setBackground(Color.black);&#xD;
    GridLayout gl = new GridLayout(3, 1);&#xD;
    quitter.addActionListener(new ActionListener() {&#xD;
&#xD;
        @Override&#xD;
        public void actionPerformed(ActionEvent arg0) {&#xD;
            // TODO Auto-generated method stub&#xD;
            System.exit(0);&#xD;
        }&#xD;
    });&#xD;
    plusMoins.addActionListener(new ActionListener() {&#xD;
&#xD;
        @Override&#xD;
        public void actionPerformed(ActionEvent e) {&#xD;
            // TODO Auto-generated method stub&#xD;
            frame.getContentPane().remove(pan);&#xD;
            initialiserPM(frame);&#xD;
        }&#xD;
    });&#xD;
    panMaster.setBackground(Color.black);&#xD;
    panPlus.setBackground(Color.black);&#xD;
    panQuitter.setBackground(Color.black);&#xD;
    panMaster.add(mastermind);&#xD;
    panPlus.add(plusMoins);&#xD;
    panQuitter.add(quitter);&#xD;
    pan.setLayout(gl);&#xD;
    pan.add(panMaster);&#xD;
    pan.add(panPlus);&#xD;
    pan.add(panQuitter);&#xD;
    frame.getContentPane().add(pan, BorderLayout.CENTER);&#xD;
    frame.setVisible(true);&#xD;
</body>
